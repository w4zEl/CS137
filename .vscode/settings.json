{
    "code-runner.runInTerminal": true,
    "code-runner.executorMap": {
        "c": "cd $dir && gcc $fileName -Wall -std=c11 -pedantic -o $fileNameWithoutExt && $dir$fileNameWithoutExt"
    },
    "terminal.integrated.env.windows": {
       "PATH": "${workspaceFolder}\\windows_scripts;${env:PATH}"
    }
}